  {
  "expo": {
    "name": "Skybunk",
    "description": "By Grebel, for Grebel.",
    "primaryColor": "#ffffff",
    "version": "5.0.1",
    "orientation": "portrait",
    "sdkVersion": "32.0.0",
    "icon": "./assets/app-icon.png",
    "ios": {
      "bundleIdentifier": "com.grebel.skybunk",
      "buildNumber": "2.2"
    },
    "android": {
      "package": "com.grebel.skybunk",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-front.png",
        "backgroundImage": "./assets/icon-back.png"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL"
      ],
      "versionCode": 4
    },
    "notification": {
      "icon": "./assets/icon-notification.png",
      "color": "#ffffff",
      "androidMode": "collapse",
      "androidCollapsedTitle": "#{unread_notifications} new posts!"
    },
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "cover",
      "image": "./assets/splash.png"
    }
  }
}
